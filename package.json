{
  "name": "@ssense/logger",
  "version": "0.1.0",
  "description": "Application and access logger",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "./node_modules/.bin/typings install",
    "compile": "rm -rf src/* && ./node_modules/.bin/tsc",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha tests/unit --recursive --compilers ts:ts-node/register --bail",
    "cover": "./node_modules/.bin/nyc --report-dir tests/coverage/ npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Groupe-Atallah/logger.git"
  },
  "keywords": [
    "logger",
    "restify"
  ],
  "author": "Cédric Maruéjol <cedric.maruejol@ssense.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Groupe-Atallah/logger/issues"
  },
  "homepage": "https://github.com/Groupe-Atallah/logger#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.1.2",
    "nyc": "^8.4.0",
    "sinon": "^1.17.6",
    "ts-node": "^1.6.1",
    "typescript": "^2.0.7",
    "typings": "^1.5.0"
  },
  "nyc": {
    "include": [
      "ts/*.ts"
    ],
    "exclude": [
      "typings",
      "node_modules",
      "ts/index.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html",
      "text-summary",
      "lcov"
    ]
  }
}
